{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "automerge": true,
  "packageRules": [
    {
      "matchPackageNames": ["flutter", "dart"],
      "groupName": "flutter"
    },
    {
      "matchManagers": ["pyenv"],
      "automerge": false
    }
  ],
  "rangeStrategy": "pin"
}
